    "@graphql-codegen/typescript-react-apollo": "2.2.4",
    "@types/react": "17.0.5",
    "graphql": "15.5.0",
    "graphql-tag": "^2.12.4",
    "hasura-cli": "2.0.0-beta.1",
    "react": "16.13.1",
    "rimraf": "3.0.2",
