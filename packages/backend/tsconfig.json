    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types/", "../../node_modules/@types/"]
  },
  "include": ["./src/**/*"]
}
