{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "web-server",
    "rootDir": "server",
    "baseUrl": "server",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["server/**/*"]
}
